iority = Priority.HIGH;\n   189→    try store.insert(issue1);\n   190→\n   191→    var issue2 = Issue.init(\"bd-low\", \"Low Priority\", 1706550000);\n   192→    issue2.priority = Priority.LOW;\n   193→    try store.insert(issue2);\n   194→\n   195→    const issues = try store.list(.{ .priority = Priority.HIGH });\n   196→    defer {\n   197→        for (issues) |*issue| {\n   198→            var i = issue.*;\n   199→            i.deinit(allocator);\n   200→        }\n   201→        allocator.free(issues);\n   202→    }\n   203→\n   204→    try std.testing.expectEqual(@as(usize, 1), issues.len);\n   205→    try std.testing.expectEqualStrings(\"bd-high\", issues[0].id);\n   206→}\n   207→\n   208→test \"IssueStore.list with limit and offset\" {\n   209→    const allocator = std.testing.allocator;\n   210→    var store = IssueStore.init(allocator, \"test_limit_offset.jsonl\");\n   211→    defer store.deinit();\n   212→\n   213→    try store.insert(Issue.init(\"bd-1\", \"Issue 1\", 1706540000));\n   214→    try store.insert(Issue.init(\"bd-2\", \"Issue 2\", 1706550000));\n   215→    try store.insert(Issue.init(\"bd-3\", \"Issue 3\", 1706560000));\n   216→    try store.insert(Issue.init(\"bd-4\", \"Issue 4\", 1706570000));\n   217→\n   218→    const issues = try store.list(.{ .limit = 2, .offset = 1 });\n   219→    defer {\n   220→        for (issues) |*issue| {\n   221→            var i = issue.*;\n   222→            i.deinit(allocator);\n   223→        }\n   224→        allocator.free(issues);\n   225→    }\n   226→\n   227→    try std.testing.expectEqual(@as(usize, 2), issues.len);\n   228→}\n   229→\n   230→test \"IssueStore dirty tracking\" {\n   231→    const allocator = std.testing.allocator;\n   232→    var store = IssueStore.init(allocator, \"test_dirty.jsonl\");\n   233→    defer store.deinit();\n   234→\n   235→    const issue = Issue.init(\"bd-dirty\", \"Dirty Test\", 1706540000);\n   236→    try store.insert(issue);\n   237→\n   238→    const dirty_ids = tr