 const custom = Status{ .custom = \"my_custom_status\" };\n    86→    try std.testing.expectEqualStrings(\"my_custom_status\", custom.toString());\n    87→}\n    88→\n    89→test \"fromString parses known statuses correctly\" {\n    90→    try std.testing.expectEqual(Status.open, Status.fromString(\"open\"));\n    91→    try std.testing.expectEqual(Status.in_progress, Status.fromString(\"in_progress\"));\n    92→    try std.testing.expectEqual(Status.blocked, Status.fromString(\"blocked\"));\n    93→    try std.testing.expectEqual(Status.deferred, Status.fromString(\"deferred\"));\n    94→    try std.testing.expectEqual(Status.closed, Status.fromString(\"closed\"));\n    95→    try std.testing.expectEqual(Status.tombstone, Status.fromString(\"tombstone\"));\n    96→    try std.testing.expectEqual(Status.pinned, Status.fromString(\"pinned\"));\n    97→}\n    98→\n    99→test \"fromString is case-insensitive\" {\n   100→    try std.testing.expectEqual(Status.open, Status.fromString(\"OPEN\"));\n   101→    try std.testing.expectEqual(Status.open, Status.fromString(\"Open\"));\n   102→    try std.testing.expectEqual(Status.open, Status.fromString(\"oPeN\"));\n   103→    try std.testing.expectEqual(Status.in_progress, Status.fromString(\"IN_PROGRESS\"));\n   104→    try std.testing.expectEqual(Status.in_progress, Status.fromString(\"In_Progress\"));\n   105→    try std.testing.expectEqual(Status.blocked, Status.fromString(\"BLOCKED\"));\n   106→    try std.testing.expectEqual(Status.closed, Status.fromString(\"CLOSED\"));\n   107→}\n   108→\n   109→test \"fromString returns custom for unknown values\" {\n   110→    const result = Status.fromString(\"unknown_status\");\n   111→    switch (result) {\n   112→        .custom => |s| try std.testing.expectEqualStrings(\"unknown_status\", s),\n   113→        else => return error.TestExpectedCustom,\n   114→    }\n   115→}\n   116→\n   117→test \"toString/fromString roundtrip for known statuses\" {\n   118→    const statuses = [