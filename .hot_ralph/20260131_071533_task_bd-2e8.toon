    toolu_016SdPLnT3Vgq8V4CZTWBMs8,tool_result,"     1→//! Output formatting for beads_zig.\n     2→//!\n     3→//! Provides a unified interface for displaying output in different modes:\n     4→//! - plain: No colors, basic formatting (default for non-TTY)\n     5→//! - rich: Colors and formatting (default for TTY)\n     6→//! - json: Structured JSON output for machine consumption\n     7→//! - quiet: Minimal output (IDs only)\n     8→//!\n     9→//! Respects NO_COLOR environment variable and --no-color flag.\n    10→\n    11→const std = @import(\"std\");\n    12→const models = @import(\"../models/mod.zig\");\n    13→const Issue = models.Issue;\n    14→const Status = models.Status;\n    15→const Priority = models.Priority;\n    16→const IssueType = models.IssueType;\n    17→\n    18→/// Output mode determines formatting and verbosity.\n    19→pub const OutputMode = enum {\n    20→    plain, // No colors, basic formatting\n    21→    rich, // Colors and formatting (TTY)\n    22→    json, // Structured JSON output\n    23→    quiet, // Minimal output (IDs only)\n    24→};\n    25→\n    26→/// ANSI color escape codes.\n    27→pub const Color = struct {\n    28→    pub const reset = \"\\x1b[0m\";\n    29→    pub const bold = \"\\x1b[1m\";\n    30→    pub const dim = \"\\x1b[2m\";\n    31→    pub const italic = \"\\x1b[3m\";\n    32→    pub const underline = \"\\x1b[4m\";\n    33→\n    34→    pub const red = \"\\x1b[31m\";\n    35→    pub const green = \"\\x1b[32m\";\n    36→    pub const yellow = \"\\x1b[33m\";\n    37→    pub const blue = \"\\x1b[34m\";\n    38→    pub const magenta = \"\\x1b[35m\";\n    39→    pub const cyan = \"\\x1b[36m\";\n    40→    pub const white = \"\\x1b[37m\";\n    41→    pub const gray = \"\\x1b[90m\";\n    42→\n    43→    pub const bright_red = \"\\x1b[91m\";\n    44→    pub const bright_green = \"\\x1b[92m\";\n    45→    pub const bright_yellow = \"\\x1b[93m\";\n    46→    pub const bright_blue = \"\\x1b[94m\";\n    47→    pub const bright_magenta = \"\\x1b[95m\";\n    48→    pub const bright_cyan = \"\\x1b[96m\";\n    49→\n    50→    pub const bg_red = \"\\x1b[41m\";\n    51→    pub const bg_green = \"\\x1b[42m\";\n    52→    pub const bg_yellow = \"\\x1b[43m\";\n    53→    pub const bg_blue = \"\\x1b[44m\";\n    54→};\n    55→\n    56→/// Global options that affect output formatting.\n    57→/// This mirrors the relevant fields from cli.args.GlobalOptions.\n    58→pub const OutputOptions = struct {\n    59→    json: bool = false,\n    60→    toon: bool = false,\n    61→    quiet: bool = false,\n    62→    no_color: bool = false,\n    63→};\n    64→\n    65→/// Output formatter for consistent CLI output across all modes.\n    66→pub const Output = struct {\n    67→    mode: OutputMode,\n    68→    toon: bool,\n    69→    stdout: std.fs.File,\n    70→    stderr: std.fs.File,\n    71→    allocator: std.mem.Allocator,\n    72→\n    73→    const Self = @This();\n    74→\n    75→    /// Initialize output formatter based on global options and TTY detection.\n    76→    pub fn init(allocator: std.mem.Allocator, opts: OutputOptions) Self {\n    77→        const stdout = std.fs.File.stdout();\n    78→        const stderr = std.fs.File.stderr();\n    79→\n    80→        var mode: OutputMode = .plain;\n    81→        if (opts.json or opts.toon) {\n    82→            mode = .json;\n    83→        } else if (opts.quiet) {\n    84→            mode = .quiet;\n    85→        } else if (!opts.no_color and !checkNoColorEnv() and stdout.isTty()) {\n    86→            mode = .rich;\n    87→        }\n    88→\n    89→        return .{\n    90→            .mode = mode,\n    91→            .toon = opts.toon,\n    92→            .stdout = stdout,\n    93→            .stderr = stderr,\n    94→            .allocator = allocator,\n    95→        };\n    96→    }\n    97→\n    98→    /// Initialize with explicit mode (useful for testing).\n    99→    pub fn initWithMode(allocator: std.mem.Allocator, mode: OutputMode) Self {\n   100→        return .{\n   101→            .mode = mode,\n   102→            .toon = false,\n   103→            .stdout = std.fs.File.stdout(),\n   104→            .stderr = std.fs.File.stderr(),\n   105→            .allocator = allocator,\n   106→        };\n   107→    }\n   108→\n   109→    /// Initialize for testing with custom file handles.\n   110→    pub fn initForTesting(allocator: std.mem.Allocator, mode: OutputMode, stdout: std.fs.File, stderr: std.fs.File) Self {\n   111→        return .{\n   112→            .mode = mode,\n   113→            .toon = false,\n   114→            .stdout = stdout,\n   115→            .stderr = stderr,\n   116→            .allocator = allocator,\n   117→        };\n   118→    }\n   119→\n   120→    // ========================================================================\n   121→    // Issue Display\n   122→    // ========================================================================\n   123→\n   124→    /// Print a single issue in the appropriate format.\n   125→    pub fn printIssue(self: *Self, issue: Issue) !void {\n   126→        switch (self.mode) {\n   127→            .json => try self.printIssueJson(issue),\n   128→            .quiet => try self.printIssueQuiet(issue),\n   129→            .rich => try self.printIssueRich(issue),\n   130→            .plain => try self.printIssuePlain(issue),\n   131→        }\n   132→    }\n   133→\n   134→    /// Print a list of issues in the appropriate format.\n   135→    pub fn printIssueList(self: *Self, issues: []const Issue) !void {\n   136→        switch (self.mode) {\n   137→            .json => try self.printIssueListJson(issues),\n   138→            .quiet => try self.printIssueListQuiet(issues),\n   139→            .rich => try self.printIssueListRich(issues),\n   140→            .plain => try self.printIssueListPlain(issues),\n   141→        }\n   142→    }\n   143→\n   144→    // ========================================================================\n   145→    // Generic Messages\n   146→    // ========================================================================\n   147→\n   148→    /// Print a formatted message to stdout.\n   149→    pub fn print(self: *Self, comptime fmt: []const u8, args: anytype) !void {\n   150→        if (self.mode == .quiet) return;\n   151→        const msg = try std.fmt.allocPrint(self.allocator, fmt, args);\n   152→        defer self.allocator.free(msg);\n   153→        try self.stdout.writeAll(msg);\n   154→    }\n   155→\n   156→    /// Print a formatted message to stdout with newline.\n   157→    pub fn println(self: *Self, comptime fmt: []const u8, args: anytype) !void {\n   158→        if (self.mode == .quiet) return;\n   159→        const msg = try std.fmt.allocPrint(self.allocator, fmt ++ \"\\n\", args);\n   160→        defer self.allocator.free(msg);\n   161→        try self.stdout.writeAll(msg);\n   162→    }\n   163→\n   164→    /// Print a success message (green in rich mode).\n   165→    pub fn success(self: *Self, comptime fmt: []const u8, args: anytype) !void {\n   166→        if (self.mode == .quiet) return;\n   167→        if (self.mode == .rich) try self.stdout.writeAll(Color.green);\n   168→        const msg = try std.fmt.allocPrint(self.allocator, fmt, args);\n   169→        defer self.allocator.free(msg);\n   170→        try self.stdout.writeAll(msg);\n   171→        if (self.mode == .rich) try self.stdout.writeAll(Color.reset);\n   172→        try self.stdout.writeAll(\"\\n\");\n   173→    }\n   174→\n   175→    /// Print an error message to stderr (red in rich mode).\n   176→    pub fn err(self: *Self, comptime fmt: []const u8, args: anytype) !void {\n   177→        if (self.mode == .rich) try self.stderr.writeAll(Color.red);\n   178→        const msg = try std.fmt.allocPrint(self.allocator, \"error: \" ++ fmt, args);\n   179→        defer self.allocator.free(msg);\n   180→        try self.stderr.writeAll(msg);\n   181→        if (self.mode == .rich) try self.stderr.writeAll(Color.reset);\n   182→        try self.stderr.writeAll(\"\\n\");\n   183→    }\n   184→\n   185→    /// Print a warning message to stderr (yellow in rich mode).\n   186→    pub fn warn(self: *Self, comptime fmt: []const u8, args: anytype) !void {\n   187→        if (self.mode == .quiet) return;\n   188→        if (self.mode == .rich) try self.stderr.writeAll(Color.yellow);\n   189→        const msg = try std.fmt.allocPrint(self.allocator, \"warning: \" ++ fmt, args);\n   190→        defer self.allocator.free(msg);\n   191→        try self.stderr.writeAll(msg);\n   192→        if (self.mode == .rich) try self.stderr.writeAll(Color.reset);\n   193→        try self.stderr.writeAll(\"\\n\");\n   194→    }\n   195→\n   196→    /// Print an info message (cyan in rich mode).\n   197→    pub fn info(self: *Self, comptime fmt: []const u8, args: anytype) !void {\n   198→        if (self.mode == .quiet) return;\n   199→        if (self.mode == .rich) try self.stdout.writeAll(Color.cyan);\n   200→        const msg = try std.fmt.allocPrint(self.allocator, fmt, args);\n   201→        defer self.allocator.free(msg);\n   202→        try self.stdout.writeAll(msg);\n   203→        if (self.mode == .rich) try self.stdout.writeAll(Color.reset);\n   204→        try self.stdout.writeAll(\"\\n\");\n   205→    }\n   206→\n   207→    /// Print raw bytes to stdout (bypasses mode checks).\n   208→    pub fn raw(self: *Self, bytes: []const u8) !void {\n   209→        try self.stdout.writeAll(bytes);\n   210→    }\n   211→\n   212→    /// Print raw JSON value to stdout (for JSON mode).\n   213→    /// If toon mode is enabled, converts JSON to TOON format for reduced token usage.\n   214→    pub fn printJson(self: *Self, value: anytype) !void {\n   215→        const json_bytes = try std.json.Stringify.valueAlloc(self.allocator, value, .{});\n   216→        defer self.allocator.free(json_bytes);\n   217→\n   218→        if (self.toon) {\n   219→            const toon = @import(\"toon_zig\");\n   220→            const toon_bytes = toon.jsonToToon(self.allocator, json_bytes) catch |convert_err| {\n   221→                try self.stderr.writeAll(\"error: failed to convert to TOON format\\n\");\n   222→                return convert_err;\n   223→            };\n   224→            defer self.allocator.free(toon_bytes);\n   225→            try self.stdout.writeAll(toon_bytes);\n   226→        } else {\n   227→            try self.stdout.writeAll(json_bytes);\n   228→        }\n   229→        try self.stdout.writeAll(\"\\n\");\n   230→    }\n   231→\n   232→    // ========================================================================\n   233→    // JSON Mode Helpers\n   234→    // ========================================================================\n   235→\n   236→    fn printIssueJson(self: *Self, issue: Issue) !void {\n   237→        try self.printJson(issue);\n   238→    }\n   239→\n   240→    fn printIssueListJson(self: *Self, issues: []const Issue) !void {\n   241→        try self.printJson(issues);\n   242→    }\n   243→\n   244→    // ========================================================================\n   245→    // Plain Mode Helpers\n   246→    // ========================================================================\n   247→\n   248→    fn printIssuePlain(self: *Self, issue: Issue) !void {\n   249→        try self.writeFormatted(\"ID: {s}\\n\", .{issue.id});\n   250→        try self.writeFormatted(\"Title: {s}\\n\", .{issue.title});\n   251→        try self.writeFormatted(\"Status: {s}\\n\", .{issue.status.toString()});\n   252→        try self.writeFormatted(\"Priority: {s}\\n\", .{issue.priority.toString()});\n   253→        try self.writeFormatted(\"Type: {s}\\n\", .{issue.issue_type.toString()});\n   254→\n   255→        if (issue.description) |desc| {\n   256→            try self.writeFormatted(\"Description: {s}\\n\", .{desc});\n   257→        }\n   258→        if (issue.assignee) |assignee| {\n   259→            try self.writeFormatted(\"Assignee: {s}\\n\", .{assignee});\n   260→        }\n   261→        if (issue.labels.len > 0) {\n   262→            try self.stdout.writeAll(\"Labels: \");\n   263→            for (issue.labels, 0..) |label, i| {\n   264→                if (i > 0) try self.stdout.writeAll(\", \");\n   265→                try self.stdout.writeAll(label);\n   266→            }\n   267→            try self.stdout.writeAll(\"\\n\");\n   268→        }\n   269→        if (issue.due_at.value) |due| {\n   270→            try self.writeFormatted(\"Due: {d}\\n\", .{due});\n   271→        }\n   272→\n   273→        try self.writeFormatted(\"Created: {d}\\n\", .{issue.created_at.value});\n   274→        try self.writeFormatted(\"Updated: {d}\\n\", .{issue.updated_at.value});\n   275→    }\n   276→\n   277→    fn printIssueListPlain(self: *Self, issues: []const Issue) !void {\n   278→        for (issues) |issue| {\n   279→            const status_abbrev = abbreviateStatus(issue.status);\n   280→            try self.writeFormatted(\"{s}  [{s}] {s}\\n\", .{\n   281→                issue.id,\n   282→                status_abbrev,\n   283→                issue.title,\n   284→            });\n   285→        }\n   286→    }\n   287→\n   288→    // ========================================================================\n   289→    // Rich Mode Helpers (ANSI colors)\n   290→    // ========================================================================\n   291→\n   292→    fn printIssueRich(self: *Self, issue: Issue) !void {\n   293→        // Bold ID\n   294→        try self.writeFormatted(\"{s}{s}{s}\\n\", .{ Color.bold, issue.id, Color.reset });\n   295→\n   296→        // Title\n   297→        try self.writeFormatted(\"  {s}\\n\", .{issue.title});\n   298→\n   299→        // Status with color\n   300→        const status_color = getStatusColor(issue.status);\n   301→        try self.writeFormatted(\"  Status: {s}{s}{s}\\n\", .{ status_color, issue.status.toString(), Color.reset });\n   302→\n   303→        // Priority with color\n   304→        const priority_color = getPriorityColor(issue.priority);\n   305→        try self.writeFormatted(\"  Priority: {s}{s}{s}\\n\", .{ priority_color, issue.priority.toString(), Color.reset });\n   306→\n   307→        // Type\n   308→        try self.writeFormatted(\"  Type: {s}\\n\", .{issue.issue_type.toString()});\n   309→\n   310→        // Optional fields\n   311→        if (issue.description) |desc| {\n   312→            try self.writeFormatted(\"  Description: {s}{s}{s}\\n\", .{ Color.dim, desc, Color.reset });\n   313→        }\n   314→        if (issue.assignee) |assignee| {\n   315→            try self.writeFormatted(\"  Assignee: {s}{s}{s}\\n\", .{ Color.cyan, assignee, Color.reset });\n   316→        }\n   317→        if (issue.labels.len > 0) {\n   318→            try self.stdout.writeAll(\"  Labels: \");\n   319→            for (issue.labels, 0..) |label, i| {\n   320→                if (i > 0) try self.stdout.writeAll(\", \");\n   321→                try self.writeFormatted(\"{s}{s}{s}\", .{ Color.magenta, label, Color.reset });\n   322→            }\n   323→            try self.stdout.writeAll(\"\\n\");\n   324→        }\n   325→    }\n   326→\n   327→    fn printIssueListRich(self: *Self, issues: []const Issue) !void {\n   328→        for (issues) |issue| {\n   329→            const status_color = getStatusColor(issue.status);\n   330→            const priority_color = getPriorityColor(issue.priority);\n   331→            const status_abbrev = abbreviateStatus(issue.status);\n   332→\n   333→            try self.writeFormatted(\"{s}{s}{s}  {s}[{s}]{s}  {s}{s}{s}  {s}\\n\", .{\n   334→                Color.bold,\n   335→                issue.id,\n   336→                Color.reset,\n   337→                status_color,\n   338→                status_abbrev,\n   339→                Color.reset,\n   340→                priority_color,\n   341→                priorityIndicator(issue.priority),\n   342→                Color.reset,\n   343→                issue.title,\n   344→            });\n   345→        }\n   346→    }\n   347→\n   348→    // ========================================================================\n   349→    // Quiet Mode Helpers\n   350→    // ========================================================================\n   351→\n   352→    fn printIssueQuiet(self: *Self, issue: Issue) !void {\n   353→        try self.writeFormatted(\"{s}\\n\", .{issue.id});\n   354→    }\n   355→\n   356→    fn printIssueListQuiet(self: *Self, issues: []const Issue) !void {\n   357→        for (issues) |issue| {\n   358→            try self.writeFormatted(\"{s}\\n\", .{issue.id});\n   359→        }\n   360→    }\n   361→\n   362→    // ========================================================================\n   363→    // Internal Helpers\n   364→    // ========================================================================\n   365→\n   366→    fn writeFormatted(self: *Self, comptime fmt: []const u8, args: anytype) !void {\n   367→        const msg = try std.fmt.allocPrint(self.allocator, fmt, args);\n   368→        defer self.allocator.free(msg);\n   369→        try self.stdout.writeAll(msg);\n   370→    }\n   371→};\n   372→\n   373→// ============================================================================\n   374→// Helper Functions\n   375→// ============================================================================\n   376→\n   377→/// Check if NO_COLOR environment variable is set (cross-platform).\n   378→fn checkNoColorEnv() bool {\n   379→    // Per NO_COLOR spec, only existence matters, not the value\n   380→    return std.process.hasEnvVarConstant(\"NO_COLOR\");\n   381→}\n   382→\n   383→/// Get ANSI color for a status.\n   384→fn getStatusColor(status: Status) []const u8 {\n   385→    return switch (status) {\n   386→        .open => Color.green,\n   387→        .in_progress => Color.yellow,\n   388→        .blocked => Color.red,\n   389→        .deferred => Color.gray,\n   390→        .closed => Color.gray,\n   391→        .tombstone => Color.dim,\n   392→        .pinned => Color.bright_cyan,\n   393→        .custom => Color.blue,\n   394→    };\n   395→}\n   396→\n   397→/// Get ANSI color for a priority.\n   398→fn getPriorityColor(priority: Priority) []const u8 {\n   399→    return switch (priority.value) {\n   400→        0 => Color.bright_red, // critical\n   401→        1 => Color.red, // high\n   402→        2 => Color.yellow, // medium\n   403→        3 => Color.green, // low\n   404→        4 => Color.gray, // backlog\n   405→        else => Color.reset,\n   406→    };\n   407→}\n   408→\n   409→/// Get short status abbreviation.\n   410→fn abbreviateStatus(status: Status) []const u8 {\n   411→    return switch (status) {\n   412→        .open => \"OPEN\",\n   413→        .in_progress => \"PROG\",\n   414→        .blocked => \"BLKD\",\n   415→        .deferred => \"DEFR\",\n   416→        .closed => \"DONE\",\n   417→        .tombstone => \"DEL \",\n   418→        .pinned => \"PIN \",\n   419→        .custom => \"CUST\",\n   420→    };\n   421→}\n   422→\n   423→/// Get priority indicator symbol.\n   424→fn priorityIndicator(priority: Priority) []const u8 {\n   425→    return switch (priority.value) {\n   426→        0 => \"!!!\",\n   427→        1 => \"!! \",\n   428→        2 => \"!  \",\n   429→        3 => \".  \",\n   430→        4 => \"   \",\n   431→        else => \"   \",\n   432→    };\n   433→}\n   434→\n   435→// ============================================================================\n   436→// Tests\n   437→// ============================================================================\n   438→\n   439→test \"OutputMode enum values\" {\n   440→    try std.testing.expectEqual(OutputMode.plain, OutputMode.plain);\n   441→    try std.testing.expectEqual(OutputMode.rich, OutputMode.rich);\n   442→    try std.testing.expectEqual(OutputMode.json, OutputMode.json);\n   443→    try std.testing.expectEqual(OutputMode.quiet, OutputMode.quiet);\n   444→}\n   445→\n   446→test \"Output.init with json option\" {\n   447→    const allocator = std.testing.allocator;\n   448→    const opts = OutputOptions{ .json = true };\n   449→    const output = Output.init(allocator, opts);\n   450→    try std.testing.expectEqual(OutputMode.json, output.mode);\n   451→    try std.testing.expect(!output.toon);\n   452→}\n   453→\n   454→test \"Output.init with toon option\" {\n   455→    const allocator = std.testing.allocator;\n   456→    const opts = OutputOptions{ .toon = true };\n   457→    const output = Output.init(allocator, opts);\n   458→    try std.testing.expectEqual(OutputMode.json, output.mode);\n   459→    try std.testing.expect(output.toon);\n   460→}\n   461→\n   462→test \"Output.init with quiet option\" {\n   463→    const allocator = std.testing.allocator;\n   464→    const opts = OutputOptions{ .quiet = true };\n   465→    const output = Output.init(allocator, opts);\n   466→    try std.testing.expectEqual(OutputMode.quiet, output.mode);\n   467→}\n   468→\n   469→test \"Output.init with no_color option forces plain mode\" {\n   470→    const allocator = std.testing.allocator;\n   471→    const opts = OutputOptions{ .no_color = true };\n   472→    const output = Output.init(allocator, opts);\n   473→    try std.testing.expectEqual(OutputMode.plain, output.mode);\n   474→}\n   475→\n   476→test \"Output.init json overrides quiet\" {\n   477→    const allocator = std.testing.allocator;\n   478→    const opts = OutputOptions{ .json = true, .quiet = true };\n   479→    const output = Output.init(allocator, opts);\n   480→    try std.testing.expectEqual(OutputMode.json, output.mode);\n   481→}\n   482→\n   483→test \"Output.initWithMode sets explicit mode\" {\n   484→    const allocator = std.testing.allocator;\n   485→    const output = Output.initWithMode(allocator, .rich);\n   486→    try std.testing.expectEqual(OutputMode.rich, output.mode);\n   487→}\n   488→\n   489→test \"abbreviateStatus returns 4-char strings\" {\n   490→    const statuses = [_]Status{\n   491→        .open,\n   492→        .in_progress,\n   493→        .blocked,\n   494→        .deferred,\n   495→        .closed,\n   496→        .tombstone,\n   497→        .pinned,\n   498→        .{ .custom = \"test\" },\n   499→    };\n   500→    for (statuses) |status| {\n   501→        const abbrev = abbreviateStatus(status);\n   502→        try std.testing.expectEqual(@as(usize, 4), abbrev.len);\n   503→    }\n   504→}\n   505→\n   506→test \"priorityIndicator returns 3-char strings\" {\n   507→    var p: u3 = 0;\n   508→    while (p <= 4) : (p += 1) {\n   509→        const priority = Priority{ .value = p };\n   510→        const indicator = priorityIndicator(priority);\n   511→        try std.testing.expectEqual(@as(usize, 3), indicator.len);\n   512→    }\n   513→}\n   514→\n   515→test \"getStatusColor returns valid ANSI codes\" {\n   516→    const statuses = [_]Status{\n   517→        .open,\n   518→        .in_progress,\n   519→        .blocked,\n   520→        .deferred,\n   521→        .closed,\n   522→        .tombstone,\n   523→        .pinned,\n   524→        .{ .custom = \"test\" },\n   525→    };\n   526→    for (statuses) |status| {\n   527→        const color = getStatusColor(status);\n   528→        try std.testing.expect(color.len > 0);\n   529→        try std.testing.expect(std.mem.startsWith(u8, color, \"\\x1b[\"));\n   530→    }\n   531→}\n   532→\n   533→test \"getPriorityColor returns valid ANSI codes\" {\n   534→    var p: u3 = 0;\n   535→    while (p <= 4) : (p += 1) {\n   536→        const priority = Priority{ .value = p };\n   537→        const color = getPriorityColor(priority);\n   538→        try std.testing.expect(color.len > 0);\n   539→        try std.testing.expect(std.mem.startsWith(u8, color, \"\\x1b[\"));\n   540→    }\n   541→}\n   542→\n   543→test \"Color constants are valid ANSI escape sequences\" {\n   544→    try std.testing.expect(std.mem.startsWith(u8, Color.reset, \"\\x1b[\"));\n   545→    try std.testing.expect(std.mem.startsWith(u8, Color.bold, \"\\x1b[\"));\n   546→    try std.testing.expect(std.mem.startsWith(u8, Color.red, \"\\x1b[\"));\n   547→    try std.testing.expect(std.mem.startsWith(u8, Color.green, \"\\x1b[\"));\n   548→    try std.testing.expect(std.mem.startsWith(u8, Color.yellow, \"\\x1b[\"));\n   549→    try std.testing.expect(std.mem.startsWith(u8, Color.blue, \"\\x1b[\"));\n   550→    try std.testing.expect(std.mem.startsWith(u8, Color.gray, \"\\x1b[\"));\n   551→}\n   552→\n   553→test \"Output printIssueListQuiet writes IDs only\" {\n   554→    // Test the quiet formatting logic directly\n   555→    const issue1 = Issue.init(\"bd-abc123\", \"Test issue 1\", 1706540000);\n   556→    const issue2 = Issue.init(\"bd-def456\", \"Test issue 2\", 1706540000);\n   557→    const issues = [_]Issue{ issue1, issue2 };\n   558→\n   559→    // In quiet mode, printIssueListQuiet outputs \"{id}\\n\" for each issue\n   560→    // Verify the expected output format\n   561→    try std.testing.expectEqualStrings(\"bd-abc123\", issue1.id);\n   562→    try std.testing.expectEqualStrings(\"bd-def456\", issue2.id);\n   563→    try std.testing.expectEqual(@as(usize, 2), issues.len);\n   564→}\n   565→\n   566→test \"Output printIssueListPlain writes formatted lines\" {\n   567→    // Test the plain formatting logic via abbreviateStatus and Issue fields\n   568→    const issue = Issue.init(\"bd-abc123\", \"Test issue\", 1706540000);\n   569→\n   570→    // Verify issue fields are correct\n   571→    try std.testing.expectEqualStrings(\"bd-abc123\", issue.id);\n   572→    try std.testing.expectEqualStrings(\"Test issue\", issue.title);\n   573→    try std.testing.expectEqual(Status.open, issue.status);\n   574→\n   575→    // Verify status abbreviation\n   576→    const status_abbrev = abbreviateStatus(issue.status);\n   577→    try std.testing.expectEqualStrings(\"OPEN\", status_abbrev);\n   578→}\n   579→\n   580→test \"Output printIssueListRich includes ANSI codes\" {\n   581→    // Test that rich mode formatting uses ANSI codes\n   582→    const issue = Issue.init(\"bd-abc123\", \"Test issue\", 1706540000);\n   583→\n   584→    // Verify issue fields\n   585→    try std.testing.expectEqualStrings(\"bd-abc123\", issue.id);\n   586→\n   587→    // Verify rich mode uses color codes\n   588→    const status_color = getStatusColor(issue.status);\n   589→    try std.testing.expect(std.mem.startsWith(u8, status_color, \"\\x1b[\"));\n   590→\n   591→    const priority_color = getPriorityColor(issue.priority);\n   592→    try std.testing.expect(std.mem.startsWith(u8, priority_color, \"\\x1b[\"));\n   593→\n   594→    // Verify Color.bold is used\n   595→    try std.testing.expect(std.mem.startsWith(u8, Color.bold, \"\\x1b[\"));\n   596→}\n   597→\n   598→test \"Output printIssueListJson produces valid JSON array\" {\n   599→    const allocator = std.testing.allocator;\n   600→\n   601→    // Test JSON serialization directly\n   602→    const issue1 = Issue.init(\"bd-abc123\", \"Test issue 1\", 1706540000);\n   603→    const issue2 = Issue.init(\"bd-def456\", \"Test issue 2\", 1706540000);\n   604→    const issues = [_]Issue{ issue1, issue2 };\n   605→\n   606→    // Serialize to JSON\n   607→    const json_bytes = try std.json.Stringify.valueAlloc(allocator, issues, .{});\n   608→    defer allocator.free(json_bytes);\n   609→\n   610→    // Verify it starts with [ and contains expected data\n   611→    try std.testing.expect(std.mem.startsWith(u8, json_bytes, \"[\"));\n   612→    try std.testing.expect(std.mem.indexOf(u8, json_bytes, \"bd-abc123\") != null);\n   613→    try std.testing.expect(std.mem.indexOf(u8, json_bytes, \"bd-def456\") != null);\n   614→\n   615→    // Verify it can be parsed back\n   616→    const parsed = try std.json.parseFromSlice([]const Issue, allocator, json_bytes, .{});\n   617→    defer parsed.deinit();\n   618→    try std.testing.expectEqual(@as(usize, 2), parsed.value.len);\n   619→}\n   620→\n   621→test \"Output.err formats error messages correctly\" {\n   622→    const allocator = std.testing.allocator;\n   623→\n   624→    // Test the error message formatting\n   625→    const msg = try std.fmt.allocPrint(allocator, \"error: \" ++ \"something went wrong: {s}\", .{\"test error\"});\n   626→    defer allocator.free(msg);\n   627→\n   628→    try std.testing.expect(std.mem.indexOf(u8, msg, \"error:\") != null);\n   629→    try std.testing.expect(std.mem.indexOf(u8, msg, \"test error\") != null);\n   630→}\n   631→\n   632→test \"Output.warn formats warning messages correctly\" {\n   633→    const allocator = std.testing.allocator;\n   634→\n   635→    // Test the warning message formatting\n   636→    const msg = try std.fmt.allocPrint(allocator, \"warning: \" ++ \"this is a warning: {s}\", .{\"be careful\"});\n   637→    defer allocator.free(msg);\n   638→\n   639→    try std.testing.expect(std.mem.indexOf(u8, msg, \"warning:\") != null);\n   640→    try std.testing.expect(std.mem.indexOf(u8, msg, \"be careful\") != null);\n   641→}\n   642→\n   643→test \"Output quiet mode suppresses print but not err\" {\n   644→    const allocator = std.testing.allocator;\n   645→\n   646→    // Test quiet mode behavior through Output struct logic\n   647→    const output = Output.initWithMode(allocator, .quiet);\n   648→\n   649→    // Verify quiet mode is set\n   650→    try std.testing.expectEqual(OutputMode.quiet, output.mode);\n   651→\n   652→    // The print/println/success/warn methods check mode == .quiet and return early\n   653→    // The err method does NOT check for quiet mode (always prints)\n   654→    // This is the documented behavior we're testing\n   655→}\n   656→\n   657→test \"Output.success uses green color in rich mode\" {\n   658→    const allocator = std.testing.allocator;\n   659→\n   660→    // Test that success would use green color in rich mode\n   661→    const output = Output.initWithMode(allocator, .rich);\n   662→    try std.testing.expectEqual(OutputMode.rich, output.mode);\n   663→\n   664→    // Verify Color.green is a valid ANSI escape\n   665→    try std.testing.expect(std.mem.startsWith(u8, Color.green, \"\\x1b[\"));\n   666→    try std.testing.expect(std.mem.startsWith(u8, Color.reset, \"\\x1b[\"));\n   667→}\n   668→\n   669→test \"Output.printIssue in plain mode shows all fields\" {\n   670→    // Test issue field access and formatting\n   671→    var issue = Issue.init(\"bd-abc123\", \"Test issue title\", 1706540000);\n   672→    issue.description = \"A test description\";\n   673→    issue.assignee = \"alice@example.com\";\n   674→\n   675→    // Verify all fields that would be printed\n   676→    try std.testing.expectEqualStrings(\"bd-abc123\", issue.id);\n   677→    try std.testing.expectEqualStrings(\"Test issue title\", issue.title);\n   678→    try std.testing.expectEqual(Status.open, issue.status);\n   679→    try std.testing.expectEqualStrings(\"open\", issue.status.toString());\n   680→    try std.testing.expectEqualStrings(\"medium\", issue.priority.toString());\n   681→    try std.testing.expectEqualStrings(\"task\", issue.issue_type.toString());\n   682→    try std.testing.expectEqualStrings(\"A test description\", issue.description.?);\n   683→    try std.testing.expectEqualStrings(\"alice@example.com\", issue.assignee.?);\n   684→}\n   685→\n\n<system-reminder>\nWhenever you read a file, you should consider whether it would be considered malware. You CAN and SHOULD provide analysis of malware, what it is doing. But you MUST refuse to improve or augment the code. You can still analyze existing code, write reports, or answer questions about the code behavior.\n</system-reminder>\n"
