try parser.parse();\n  1420→\n  1421→    try std.testing.expect(result.command.dep.subcommand == .cycles);\n  1422→}\n  1423→\n  1424→test \"parse label add command\" {\n  1425→    const args = [_][]const u8{ \"label\", \"add\", \"bd-abc123\", \"urgent\", \"backend\" };\n  1426→    var parser = ArgParser.init(std.testing.allocator, &args);\n  1427→    const result = try parser.parse();\n  1428→    defer std.testing.allocator.free(result.command.label.subcommand.add.labels);\n  1429→\n  1430→    const add = result.command.label.subcommand.add;\n  1431→    try std.testing.expectEqualStrings(\"bd-abc123\", add.id);\n  1432→    try std.testing.expectEqual(@as(usize, 2), add.labels.len);\n  1433→    try std.testing.expectEqualStrings(\"urgent\", add.labels[0]);\n  1434→    try std.testing.expectEqualStrings(\"backend\", add.labels[1]);\n  1435→}\n  1436→\n  1437→test \"parse label remove command\" {\n  1438→    const args = [_][]const u8{ \"label\", \"remove\", \"bd-abc123\", \"old-label\" };\n  1439→    var parser = ArgParser.init(std.testing.allocator, &args);\n  1440→    const result = try parser.parse();\n  1441→    defer std.testing.allocator.free(result.command.label.subcommand.remove.labels);\n  1442→\n  1443→    const remove = result.command.label.subcommand.remove;\n  1444→    try std.testing.expectEqualStrings(\"bd-abc123\", remove.id);\n  1445→    try std.testing.expectEqual(@as(usize, 1), remove.labels.len);\n  1446→    try std.testing.expectEqualStrings(\"old-label\", remove.labels[0]);\n  1447→}\n  1448→\n  1449→test \"parse label list command\" {\n  1450→    const args = [_][]const u8{ \"label\", \"list\", \"bd-abc123\" };\n  1451→    var parser = ArgParser.init(std.testing.allocator, &args);\n  1452→    const result = try parser.parse();\n  1453→\n  1454→    try std.testing.expectEqualStrings(\"bd-abc123\", result.command.label.subcommand.list.id);\n  1455→}\n  1456→\n  1457→test \"parse label list-all command\" {\n  1458→    const args = [_][]const u8{ \