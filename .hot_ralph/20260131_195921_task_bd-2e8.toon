st_get.jsonl\");\n    45→    defer store.deinit();\n    46→\n    47→    const original = Issue.init(\"bd-test2\", \"Get Test\", 1706540000);\n    48→    try store.insert(original);\n    49→\n    50→    var retrieved = (try store.get(\"bd-test2\")).?;\n    51→    defer retrieved.deinit(allocator);\n    52→\n    53→    try std.testing.expectEqualStrings(\"bd-test2\", retrieved.id);\n    54→    try std.testing.expectEqualStrings(\"Get Test\", retrieved.title);\n    55→    try std.testing.expectEqual(Status.open, retrieved.status);\n    56→    try std.testing.expectEqual(Priority.MEDIUM, retrieved.priority);\n    57→}\n    58→\n    59→test \"IssueStore.get returns null for missing issue\" {\n    60→    const allocator = std.testing.allocator;\n    61→    var store = IssueStore.init(allocator, \"test_missing.jsonl\");\n    62→    defer store.deinit();\n    63→\n    64→    const result = try store.get(\"bd-nonexistent\");\n    65→    try std.testing.expect(result == null);\n    66→}\n    67→\n    68→test \"IssueStore.update modifies fields\" {\n    69→    const allocator = std.testing.allocator;\n    70→    var store = IssueStore.init(allocator, \"test_update.jsonl\");\n    71→    defer store.deinit();\n    72→\n    73→    const issue = Issue.init(\"bd-update\", \"Original Title\", 1706540000);\n    74→    try store.insert(issue);\n    75→\n    76→    try store.update(\"bd-update\", .{\n    77→        .title = \"Updated Title\",\n    78→        .status = .in_progress,\n    79→        .priority = Priority.HIGH,\n    80→    }, 1706550000);\n    81→\n    82→    var updated = (try store.get(\"bd-update\")).?;\n    83→    defer updated.deinit(allocator);\n    84→\n    85→    try std.testing.expectEqualStrings(\"Updated Title\", updated.title);\n    86→    try std.testing.expectEqual(@as(i64, 1706550000), updated.updated_at.value);\n    87→}\n    88→\n    89→test \"IssueStore.update returns error for missing issue\" {\n    90→    const allocator = std.testing.allocator;\n    91→    var store = IssueStore.init(allocator, \"test_update_missing.jsonl\");\n    92→    defer store.deinit();\n    93→\n    94→    const result = store.update(\"bd-missing\", .{ .title = \"New\" }, 1706550000);\n    95→    try std.testing.expectError(IssueStoreError.IssueNotFound, result);\n    96→}\n    97→\n    98→test \"IssueStore.delete sets tombstone status\" {\n    99→    const allocator = std.testing.allocator;\n   100→    var store = IssueStore.init(allocator, \"test_delete.jsonl\");\n   101→    defer store.deinit();\n   102→\n   103→    const issue = Issue.init(\"bd-delete\", \"To Delete\", 1706540000);\n   104→    try store.insert(issue);\n   105→    try store.delete(\"bd-delete\", 1706550000);\n   106→\n   107→    var deleted = (try store.get(\"bd-delete\")).?;\n   108→    defer deleted.deinit(allocator);\n   109→\n   110→    try std.testing.expectEqual(Status.tombstone, deleted.status);\n   111→}\n   112→\n   113→test \"IssueStore.list returns issues\" {\n   114→    const allocator = std.testing.allocator;\n   115→    var store = IssueStore.init(allocator, \"test_list.jsonl\");\n   116→    defer store.deinit();\n   117→\n   118→    try store.insert(Issue.init(\"bd-list1\", \"Issue 1\", 1706540000));\n   119→    try store.insert(Issue.init(\"bd-list2\", \"Issue 2\", 1706550000));\n   120→    try store.insert(Issue.init(\"bd-list3\", \"Issue 3\", 1706560000));\n   121→\n   122→    const issues = try store.list(.{});\n   123→    defer {\n   124→        for (issues) |*issue| {\n   125→            var i = issue.*;\n   126→            i.deinit(allocator);\n   127→        }\n   128→        allocator.free(issues);\n   129→    }\n   130→\n   131→    try std.testing.expectEqual(@as(usize, 3), issues.len);\n   132→}\n   133→\n   134→test \"IssueStore.list excludes tombstones by default\" {\n   135→    const allocator = std.testing.allocator;\n   136→    var store = IssueStore.init(allocator, \"test_tombstone.jsonl\");\n   137→    defer store.deinit();\n   138→\n   