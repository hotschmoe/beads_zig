e());\n  1592→}\n  1593→\n  1594→test \"parse completions command missing shell returns error\" {\n  1595→    const args = [_][]const u8{\"completions\"};\n  1596→    var parser = ArgParser.init(std.testing.allocator, &args);\n  1597→\n  1598→    try std.testing.expectError(error.MissingRequiredArgument, parser.parse());\n  1599→}\n  1600→\n  1601→test \"parse config list (default)\" {\n  1602→    const args = [_][]const u8{\"config\"};\n  1603→    var parser = ArgParser.init(std.testing.allocator, &args);\n  1604→    const result = try parser.parse();\n  1605→\n  1606→    try std.testing.expect(result.command == .config);\n  1607→    try std.testing.expect(result.command.config.subcommand == .list);\n  1608→}\n  1609→\n  1610→test \"parse config get\" {\n  1611→    const args = [_][]const u8{ \"config\", \"get\", \"id.prefix\" };\n  1612→    var parser = ArgParser.init(std.testing.allocator, &args);\n  1613→    const result = try parser.parse();\n  1614→\n  1615→    try std.testing.expectEqualStrings(\"id.prefix\", result.command.config.subcommand.get.key);\n  1616→}\n  1617→\n  1618→test \"parse config set\" {\n  1619→    const args = [_][]const u8{ \"config\", \"set\", \"id.prefix\", \"proj\" };\n  1620→    var parser = ArgParser.init(std.testing.allocator, &args);\n  1621→    const result = try parser.parse();\n  1622→\n  1623→    const set = result.command.config.subcommand.set;\n  1624→    try std.testing.expectEqualStrings(\"id.prefix\", set.key);\n  1625→    try std.testing.expectEqualStrings(\"proj\", set.value);\n  1626→}\n  1627→\n  1628→test \"command aliases work\" {\n  1629→    // Test 'add' as alias for 'create'\n  1630→    {\n  1631→        const args = [_][]const u8{ \"add\", \"Test title\" };\n  1632→        var parser = ArgParser.init(std.testing.allocator, &args);\n  1633→        const result = try parser.parse();\n  1634→        try std.testing.expect(result.command == .create);\n  1635→    }\n  1636→\n  1637→    // Test 'ls' as alias for 'list'