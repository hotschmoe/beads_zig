endents(show_args.id);\n    55→    defer graph.freeDependencies(dependents);\n    56→\n    57→    if (structured_output) {\n    58→        var depends_on_ids: ?[][]const u8 = null;\n    59→        var blocks_ids: ?[][]const u8 = null;\n    60→\n    61→        if (deps.len > 0) {\n    62→            depends_on_ids = try allocator.alloc([]const u8, deps.len);\n    63→            for (deps, 0..) |dep, i| {\n    64→                depends_on_ids.?[i] = dep.depends_on_id;\n    65→            }\n    66→        }\n    67→\n    68→        if (dependents.len > 0) {\n    69→            blocks_ids = try allocator.alloc([]const u8, dependents.len);\n    70→            for (dependents, 0..) |dep, i| {\n    71→                blocks_ids.?[i] = dep.issue_id;\n    72→            }\n    73→        }\n    74→\n    75→        defer {\n    76→            if (depends_on_ids) |ids| allocator.free(ids);\n    77→            if (blocks_ids) |ids| allocator.free(ids);\n    78→        }\n    79→\n    80→        try ctx.output.printJson(ShowResult{\n    81→            .success = true,\n    82→            .issue = issue,\n    83→            .depends_on = depends_on_ids,\n    84→            .blocks = blocks_ids,\n    85→        });\n    86→    } else {\n    87→        try ctx.output.printIssue(issue);\n    88→\n    89→        if (deps.len > 0) {\n    90→            try ctx.output.print(\"\\nDepends on:\\n\", .{});\n    91→            for (deps) |dep| {\n    92→                try ctx.output.print(\"  - {s}\\n\", .{dep.depends_on_id});\n    93→            }\n    94→        }\n    95→\n    96→        if (dependents.len > 0) {\n    97→            try ctx.output.print(\"\\nBlocks:\\n\", .{});\n    98→            for (dependents) |dep| {\n    99→                try ctx.output.print(\"  - {s}\\n\", .{dep.issue_id});\n   100→            }\n   101→        }\n   102→    }\n   103→}\n   104→\n   105→// --- Tests ---\n   106→\n   107→test \"ShowError enum exists\" {\n   108→    const err: ShowError = ShowE