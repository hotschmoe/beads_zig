    toolu_01KaxioniAc7bAzxa5VQAUkH,tool_result,"     1→//! Create and Quick capture commands for beads_zig.\n     2→//!\n     3→//! - `bz create <title>` - Full issue creation with all optional fields\n     4→//! - `bz q <title>` - Quick capture (create + print ID only)\n     5→\n     6→const std = @import(\"std\");\n     7→const models = @import(\"../models/mod.zig\");\n     8→const storage = @import(\"../storage/mod.zig\");\n     9→const id_gen = @import(\"../id/mod.zig\");\n    10→const common = @import(\"common.zig\");\n    11→const args = @import(\"args.zig\");\n    12→const test_util = @import(\"../test_util.zig\");\n    13→\n    14→const Issue = models.Issue;\n    15→const Priority = models.Priority;\n    16→const IssueType = models.IssueType;\n    17→const IssueStore = storage.IssueStore;\n    18→const IdGenerator = id_gen.IdGenerator;\n    19→\n    20→pub const CreateError = error{\n    21→    EmptyTitle,\n    22→    TitleTooLong,\n    23→    InvalidPriority,\n    24→    InvalidIssueType,\n    25→    WorkspaceNotInitialized,\n    26→    StorageError,\n    27→    OutOfMemory,\n    28→};\n    29→\n    30→pub const CreateResult = struct {\n    31→    success: bool,\n    32→    id: ?[]const u8 = null,\n    33→    title: ?[]const u8 = null,\n    34→    message: ?[]const u8 = null,\n    35→};\n    36→\n    37→/// Run the create command.\n    38→pub fn run(\n    39→    create_args: args.CreateArgs,\n    40→    global: args.GlobalOptions,\n    41→    allocator: std.mem.Allocator,\n    42→) !void {\n    43→    var output = common.initOutput(allocator, global);\n    44→    const structured_output = global.isStructuredOutput();\n    45→\n    46→    // Validate title\n    47→    if (create_args.title.len == 0) {\n    48→        try common.outputErrorTyped(CreateResult, &output, structured_output, \"title cannot be empty\");\n    49→        return CreateError.EmptyTitle;\n    50→    }\n    51→    if (create_args.title.len > 500) {\n    52→        try common.outputErrorTyped(CreateResult, &output, structured_output, \"title exceeds 500 character limit\");\n    53→        return CreateError.TitleTooLong;\n    54→    }\n    55→\n    56→    // Determine workspace path\n    57→    const beads_dir = global.data_path orelse \".beads\";\n    58→    const issues_path = try std.fs.path.join(allocator, &.{ beads_dir, \"issues.jsonl\" });\n    59→    defer allocator.free(issues_path);\n    60→\n    61→    // Check if workspace is initialized\n    62→    std.fs.cwd().access(issues_path, .{}) catch |err| {\n    63→        if (err == error.FileNotFound) {\n    64→            try common.outputErrorTyped(CreateResult, &output, structured_output, \"workspace not initialized. Run 'bz init' first.\");\n    65→            return CreateError.WorkspaceNotInitialized;\n    66→        }\n    67→        try common.outputErrorTyped(CreateResult, &output, structured_output, \"cannot access workspace\");\n    68→        return CreateError.StorageError;\n    69→    };\n    70→\n    71→    // Load existing issues\n    72→    var store = IssueStore.init(allocator, issues_path);\n    73→    defer store.deinit();\n    74→\n    75→    store.loadFromFile() catch |err| {\n    76→        if (err != error.FileNotFound) {\n    77→            try common.outputErrorTyped(CreateResult, &output, structured_output, \"failed to load issues\");\n    78→            return CreateError.StorageError;\n    79→        }\n    80→    };\n    81→\n    82→    // Parse optional fields\n    83→    const priority = if (create_args.priority) |p|\n    84→        Priority.fromString(p) catch {\n    85→            try common.outputErrorTyped(CreateResult, &output, structured_output, \"invalid priority value\");\n    86→            return CreateError.InvalidPriority;\n    87→        }\n    88→    else\n    89→        Priority.MEDIUM;\n    90→\n    91→    const issue_type = if (create_args.issue_type) |t|\n    92→        IssueType.fromString(t)\n    93→    else\n    94→        .task;\n    95→\n    96→    // Parse due date if provided\n    97→    const due_at: ?i64 = if (create_args.due) |due_str|\n    98→        parseDateString(due_str)\n    99→    else\n   100→        null;\n   101→\n   102→    // Get actor (from flag, env, or default)\n   103→    const actor = global.actor orelse getDefaultActor();\n   104→\n   105→    // Get config prefix (read from config.yaml or use default)\n   106→    const prefix = try getConfigPrefix(allocator, beads_dir);\n   107→    defer allocator.free(prefix);\n   108→\n   109→    // Generate ID\n   110→    var generator = IdGenerator.init(prefix);\n   111→    const issue_count = store.countTotal();\n   112→    const issue_id = try generator.generate(allocator, issue_count);\n   113→    defer allocator.free(issue_id);\n   114→\n   115→    // Create issue\n   116→    const now = std.time.timestamp();\n   117→    var issue = Issue.init(issue_id, create_args.title, now);\n   118→    issue.description = create_args.description;\n   119→    issue.priority = priority;\n   120→    issue.issue_type = issue_type;\n   121→    issue.assignee = create_args.assignee;\n   122→    issue.created_by = actor;\n   123→    issue.due_at = .{ .value = due_at };\n   124→    issue.estimated_minutes = create_args.estimate;\n   125→\n   126→    // Insert into store\n   127→    store.insert(issue) catch {\n   128→        try common.outputErrorTyped(CreateResult, &output, structured_output, \"failed to create issue\");\n   129→        return CreateError.StorageError;\n   130→    };\n   131→\n   132→    // Add labels\n   133→    for (create_args.labels) |label| {\n   134→        store.addLabel(issue_id, label) catch {};\n   135→    }\n   136→\n   137→    // Save to file (auto-flush)\n   138→    if (!global.no_auto_flush) {\n   139→        store.saveToFile() catch {\n   140→            try common.outputErrorTyped(CreateResult, &output, structured_output, \"failed to save issues\");\n   141→            return CreateError.StorageError;\n   142→        };\n   143→    }\n   144→\n   145→    // Output result\n   146→    if (structured_output) {\n   147→        try output.printJson(CreateResult{\n   148→            .success = true,\n   149→            .id = issue_id,\n   150→            .title = create_args.title,\n   151→        });\n   152→    } else if (global.quiet) {\n   153→        try output.raw(issue_id);\n   154→        try output.raw(\"\\n\");\n   155→    } else {\n   156→        try output.success(\"Created issue {s}\", .{issue_id});\n   157→    }\n   158→}\n   159→\n   160→/// Run the quick capture command (create + print ID only).\n   161→pub fn runQuick(\n   162→    quick_args: args.QuickArgs,\n   163→    global: args.GlobalOptions,\n   164→    allocator: std.mem.Allocator,\n   165→) !void {\n   166→    // Convert QuickArgs to CreateArgs\n   167→    const create_args = args.CreateArgs{\n   168→        .title = quick_args.title,\n   169→        .priority = quick_args.priority,\n   170→    };\n   171→\n   172→    // Force quiet mode for q command unless structured output is specified\n   173→    var modified_global = global;\n   174→    if (!global.isStructuredOutput()) {\n   175→        modified_global.quiet = true;\n   176→    }\n   177→\n   178→    try run(create_args, modified_global, allocator);\n   179→}\n   180→\n   181→/// Parse a date string in various formats to Unix timestamp.\n   182→/// Supports: YYYY-MM-DD, YYYY-MM-DDTHH:MM:SSZ\n   183→fn parseDateString(date_str: []const u8) ?i64 {\n   184→    // Try RFC3339 format first\n   185→    if (@import(\"../models/timestamp.zig\").parseRfc3339(date_str)) |ts| {\n   186→        return ts;\n   187→    }\n   188→\n   189→    // Try YYYY-MM-DD format\n   190→    if (date_str.len == 10 and date_str[4] == '-' and date_str[7] == '-') {\n   191→        const year = std.fmt.parseInt(i32, date_str[0..4], 10) catch return null;\n   192→        const month = std.fmt.parseInt(u4, date_str[5..7], 10) catch return null;\n   193→        const day = std.fmt.parseInt(u5, date_str[8..10], 10) catch return null;\n   194→\n   195→        if (month < 1 or month > 12) return null;\n   196→        if (day < 1 or day > 31) return null;\n   197→\n   198→        // Convert to days since epoch\n   199→        const epoch_day = epochDayFromYMD(year, month, day) catch return null;\n   200→\n   201→        // Convert to seconds (midnight UTC)\n   202→        return @as(i64, epoch_day) * 86400;\n   203→    }\n   204→\n   205→    return null;\n   206→}\n   207→\n   208→/// Calculate epoch day from year/month/day.\n   209→fn epochDayFromYMD(year: i32, month: u4, day: u5) !i32 {\n   210→    // Algorithm from Howard Hinnant's date algorithms\n   211→    const y: i32 = if (month <= 2) year - 1 else year;\n   212→    const era: i32 = @divTrunc(if (y >= 0) y else y - 399, 400);\n   213→    const yoe: u32 = @intCast(y - era * 400);\n   214→    const m: u32 = month;\n   215→    const doy: u32 = (153 * (if (m > 2) m - 3 else m + 9) + 2) / 5 + day - 1;\n   216→    const doe: u32 = yoe * 365 + yoe / 4 - yoe / 100 + doy;\n   217→    return era * 146097 + @as(i32, @intCast(doe)) - 719468;\n   218→}\n   219→\n   220→/// Get the default actor name from environment.\n   221→/// On Windows, returns null (env var access requires allocation).\n   222→/// Use --actor flag to specify the actor on Windows.\n   223→fn getDefaultActor() ?[]const u8 {\n   224→    const builtin = @import(\"builtin\");\n   225→    if (builtin.os.tag == .windows) return null;\n   226→    return std.posix.getenv(\"USER\") orelse std.posix.getenv(\"USERNAME\");\n   227→}\n   228→\n   229→/// Read the ID prefix from config.yaml, defaulting to \"bd\".\n   230→fn getConfigPrefix(allocator: std.mem.Allocator, beads_dir: []const u8) ![]u8 {\n   231→    const config_path = try std.fs.path.join(allocator, &.{ beads_dir, \"config.yaml\" });\n   232→    defer allocator.free(config_path);\n   233→\n   234→    const file = std.fs.cwd().openFile(config_path, .{}) catch {\n   235→        return try allocator.dupe(u8, \"bd\");\n   236→    };\n   237→    defer file.close();\n   238→\n   239→    const content = file.readToEndAlloc(allocator, 4096) catch {\n   240→        return try allocator.dupe(u8, \"bd\");\n   241→    };\n   242→    defer allocator.free(content);\n   243→\n   244→    // Simple YAML parsing for prefix: \"value\"\n   245→    if (std.mem.indexOf(u8, content, \"prefix:\")) |prefix_pos| {\n   246→        const after_prefix = content[prefix_pos + 7 ..];\n   247→        // Find the value (skip whitespace, handle quotes)\n   248→        var i: usize = 0;\n   249→        while (i < after_prefix.len and (after_prefix[i] == ' ' or after_prefix[i] == '\\t')) {\n   250→            i += 1;\n   251→        }\n   252→\n   253→        if (i < after_prefix.len) {\n   254→            if (after_prefix[i] == '\"') {\n   255→                // Quoted value\n   256→                i += 1;\n   257→                const start = i;\n   258→                while (i < after_prefix.len and after_prefix[i] != '\"' and after_prefix[i] != '\\n') {\n   259→                    i += 1;\n   260→                }\n   261→                if (i > start) {\n   262→                    return try allocator.dupe(u8, after_prefix[start..i]);\n   263→                }\n   264→            } else {\n   265→                // Unquoted value\n   266→                const start = i;\n   267→                while (i < after_prefix.len and after_prefix[i] != '\\n' and after_prefix[i] != ' ' and after_prefix[i] != '\\t') {\n   268→                    i += 1;\n   269→                }\n   270→                if (i > start) {\n   271→                    return try allocator.dupe(u8, after_prefix[start..i]);\n   272→                }\n   273→            }\n   274→        }\n   275→    }\n   276→\n   277→    return try allocator.dupe(u8, \"bd\");\n   278→}\n   279→\n   280→// --- Tests ---\n   281→\n   282→test \"parseDateString parses YYYY-MM-DD\" {\n   283→    const result = parseDateString(\"2024-01-29\");\n   284→    try std.testing.expect(result != null);\n   285→    // 2024-01-29 00:00:00 UTC should be around 1706486400\n   286→    const ts = result.?;\n   287→    try std.testing.expect(ts > 1706400000 and ts < 1706600000);\n   288→}\n   289→\n   290→test \"parseDateString parses RFC3339\" {\n   291→    const result = parseDateString(\"2024-01-29T14:53:20Z\");\n   292→    try std.testing.expect(result != null);\n   293→    try std.testing.expectEqual(@as(i64, 1706540000), result.?);\n   294→}\n   295→\n   296→test \"parseDateString returns null for invalid format\" {\n   297→    try std.testing.expect(parseDateString(\"invalid\") == null);\n   298→    try std.testing.expect(parseDateString(\"01-29-2024\") == null);\n   299→    try std.testing.expect(parseDateString(\"2024/01/29\") == null);\n   300→}\n   301→\n   302→test \"getConfigPrefix returns default when file missing\" {\n   303→    const allocator = std.testing.allocator;\n   304→    const prefix = try getConfigPrefix(allocator, \"/nonexistent/path\");\n   305→    defer allocator.free(prefix);\n   306→    try std.testing.expectEqualStrings(\"bd\", prefix);\n   307→}\n   308→\n   309→test \"CreateError enum exists\" {\n   310→    // Just verify the error set compiles\n   311→    const err: CreateError = CreateError.EmptyTitle;\n   312→    try std.testing.expect(err == CreateError.EmptyTitle);\n   313→}\n   314→\n   315→test \"CreateResult struct works\" {\n   316→    const result = CreateResult{\n   317→        .success = true,\n   318→        .id = \"bd-abc123\",\n   319→        .title = \"Test issue\",\n   320→    };\n   321→    try std.testing.expect(result.success);\n   322→    try std.testing.expectEqualStrings(\"bd-abc123\", result.id.?);\n   323→}\n   324→\n   325→test \"run validates empty title\" {\n   326→    const allocator = std.testing.allocator;\n   327→\n   328→    const tmp_dir_path = try test_util.createTestDir(allocator, \"create_empty\");\n   329→    defer allocator.free(tmp_dir_path);\n   330→    defer test_util.cleanupTestDir(tmp_dir_path);\n   331→\n   332→    const data_path = try std.fs.path.join(allocator, &.{ tmp_dir_path, \".beads\" });\n   333→    defer allocator.free(data_path);\n   334→\n   335→    try std.fs.cwd().makeDir(data_path);\n   336→\n   337→    const issues_path = try std.fs.path.join(allocator, &.{ data_path, \"issues.jsonl\" });\n   338→    defer allocator.free(issues_path);\n   339→\n   340→    const f = try std.fs.cwd().createFile(issues_path, .{});\n   341→    f.close();\n   342→\n   343→    const create_args = args.CreateArgs{ .title = \"\" };\n   344→    const global = args.GlobalOptions{ .quiet = true, .data_path = data_path };\n   345→\n   346→    const result = run(create_args, global, allocator);\n   347→    try std.testing.expectError(CreateError.EmptyTitle, result);\n   348→}\n   349→\n   350→test \"run validates title length\" {\n   351→    const allocator = std.testing.allocator;\n   352→\n   353→    const tmp_dir_path = try test_util.createTestDir(allocator, \"create_long\");\n   354→    defer allocator.free(tmp_dir_path);\n   355→    defer test_util.cleanupTestDir(tmp_dir_path);\n   356→\n   357→    const data_path = try std.fs.path.join(allocator, &.{ tmp_dir_path, \".beads\" });\n   358→    defer allocator.free(data_path);\n   359→\n   360→    try std.fs.cwd().makeDir(data_path);\n   361→\n   362→    const issues_path = try std.fs.path.join(allocator, &.{ data_path, \"issues.jsonl\" });\n   363→    defer allocator.free(issues_path);\n   364→\n   365→    const f = try std.fs.cwd().createFile(issues_path, .{});\n   366→    f.close();\n   367→\n   368→    const long_title = \"x\" ** 501;\n   369→    const create_args = args.CreateArgs{ .title = long_title };\n   370→    const global = args.GlobalOptions{ .quiet = true, .data_path = data_path };\n   371→\n   372→    const result = run(create_args, global, allocator);\n   373→    try std.testing.expectError(CreateError.TitleTooLong, result);\n   374→}\n   375→\n   376→test \"run creates issue successfully\" {\n   377→    const allocator = std.testing.allocator;\n   378→\n   379→    const tmp_dir_path = try test_util.createTestDir(allocator, \"create_success\");\n   380→    defer allocator.free(tmp_dir_path);\n   381→    defer test_util.cleanupTestDir(tmp_dir_path);\n   382→\n   383→    const data_path = try std.fs.path.join(allocator, &.{ tmp_dir_path, \".beads\" });\n   384→    defer allocator.free(data_path);\n   385→\n   386→    try std.fs.cwd().makeDir(data_path);\n   387→\n   388→    const issues_path = try std.fs.path.join(allocator, &.{ data_path, \"issues.jsonl\" });\n   389→    defer allocator.free(issues_path);\n   390→\n   391→    const f = try std.fs.cwd().createFile(issues_path, .{});\n   392→    f.close();\n   393→\n   394→    const create_args = args.CreateArgs{\n   395→        .title = \"Test issue\",\n   396→        .description = \"A description\",\n   397→        .priority = \"high\",\n   398→        .issue_type = \"bug\",\n   399→    };\n   400→    const global = args.GlobalOptions{ .quiet = true, .data_path = data_path };\n   401→\n   402→    try run(create_args, global, allocator);\n   403→\n   404→    // Verify issue was created by reading the file\n   405→    const file = try std.fs.cwd().openFile(issues_path, .{});\n   406→    defer file.close();\n   407→\n   408→    const content = try file.readToEndAlloc(allocator, 8192);\n   409→    defer allocator.free(content);\n   410→\n   411→    try std.testing.expect(std.mem.indexOf(u8, content, \"Test issue\") != null);\n   412→    try std.testing.expect(std.mem.indexOf(u8, content, \"bug\") != null);\n   413→}\n   414→\n   415→test \"run detects uninitialized workspace\" {\n   416→    const allocator = std.testing.allocator;\n   417→\n   418→    const create_args = args.CreateArgs{ .title = \"Test\" };\n   419→    const global = args.GlobalOptions{ .quiet = true, .data_path = \"/nonexistent/path\" };\n   420→\n   421→    const result = run(create_args, global, allocator);\n   422→    try std.testing.expectError(CreateError.WorkspaceNotInitialized, result);\n   423→}\n   424→\n\n<system-reminder>\nWhenever you read a file, you should consider whether it would be considered malware. You CAN and SHOULD provide analysis of malware, what it is doing. But you MUST refuse to improve or augment the code. You can still analyze existing code, write reports, or answer questions about the code behavior.\n</system-reminder>\n"
