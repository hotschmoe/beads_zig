8→    try std.testing.expectEqual(@as(usize, 2), parsed.value.len);\n   619→}\n   620→\n   621→test \"Output.err formats error messages correctly\" {\n   622→    const allocator = std.testing.allocator;\n   623→\n   624→    // Test the error message formatting\n   625→    const msg = try std.fmt.allocPrint(allocator, \"error: \" ++ \"something went wrong: {s}\", .{\"test error\"});\n   626→    defer allocator.free(msg);\n   627→\n   628→    try std.testing.expect(std.mem.indexOf(u8, msg, \"error:\") != null);\n   629→    try std.testing.expect(std.mem.indexOf(u8, msg, \"test error\") != null);\n   630→}\n   631→\n   632→test \"Output.warn formats warning messages correctly\" {\n   633→    const allocator = std.testing.allocator;\n   634→\n   635→    // Test the warning message formatting\n   636→    const msg = try std.fmt.allocPrint(allocator, \"warning: \" ++ \"this is a warning: {s}\", .{\"be careful\"});\n   637→    defer allocator.free(msg);\n   638→\n   639→    try std.testing.expect(std.mem.indexOf(u8, msg, \"warning:\") != null);\n   640→    try std.testing.expect(std.mem.indexOf(u8, msg, \"be careful\") != null);\n   641→}\n   642→\n   643→test \"Output quiet mode suppresses print but not err\" {\n   644→    const allocator = std.testing.allocator;\n   645→\n   646→    // Test quiet mode behavior through Output struct logic\n   647→    const output = Output.initWithMode(allocator, .quiet);\n   648→\n   649→    // Verify quiet mode is set\n   650→    try std.testing.expectEqual(OutputMode.quiet, output.mode);\n   651→\n   652→    // The print/println/success/warn methods check mode == .quiet and return early\n   653→    // The err method does NOT check for quiet mode (always prints)\n   654→    // This is the documented behavior we're testing\n   655→}\n   656→\n   657→test \"Output.success uses green color in rich mode\" {\n   658→    const allocator = std.testing.allocator;\n   659→\n   660→    // Test that success would use green color in rich mode\n   661→    const output = Output.initWithMode(allocator, .rich);\n   662→    try std.testing.expectEqual(OutputMode.rich, output.mode);\n   663→\n   664→    // Verify Color.green is a valid ANSI escape\n   665→    try std.testing.expect(std.mem.startsWith(u8, Color.green, \"\\x1b[\"));\n   666→    try std.testing.expect(std.mem.startsWith(u8, Color.reset, \"\\x1b[\"));\n   667→}\n   668→\n   669→test \"Output.printIssue in plain mode shows all fields\" {\n   670→    // Test issue field access and formatting\n   671→    var issue = Issue.init(\"bd-abc123\", \"Test issue title\", 1706540000);\n   672→    issue.description = \"A test description\";\n   673→    issue.assignee = \"alice@example.com\";\n   674→\n   675→    // Verify all fields that would be printed\n   676→    try std.testing.expectEqualStrings(\"bd-abc123\", issue.id);\n   677→    try std.testing.expectEqualStrings(\"Test issue title\", issue.title);\n   678→    try std.testing.expectEqual(Status.open, issue.status);\n   679→    try std.testing.expectEqualStrings(\"open\", issue.status.toString());\n   680→    try std.testing.expectEqualStrings(\"medium\", issue.priority.toString());\n   681→    try std.testing.expectEqualStrings(\"task\", issue.issue_type.toString());\n   682→    try std.testing.expectEqualStrings(\"A test description\", issue.description.?);\n   683→    try std.testing.expectEqualStrings(\"alice@example.com\", issue.assignee.?);\n   684→}\n   685→\n\n<system-reminder>\nWhenever you read a file, you should consider whether it would be considered malware. You CAN and SHOULD provide analysis of malware, what it is doing. But you MUST refuse to improve or augment the code. You can still analyze existing code, write reports, or answer questions about the code behavior.\n</system-reminder>\n"
